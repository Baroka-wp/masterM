<div id="user_element" data-user-id="<%= current_user.name %>"></div>
<div id="room-id" data-room-id="<%= @classroom.id %>"></div>
<div class="" style="max-height:100vh">
  <%= render 'classrooms/shared/sidenav' %>
  <div class="main-content" id="panel">
    <div class="container-fluid mt-2">
        <div class="row">
          <div class="col-lg-4 col-md-4 order-xl-2 mt-4">
            <%= render 'classrooms/shared/chat_card' %>
          </div>
          <div id="courses" class="col-lg-8 col-md-4 order-xl-1 mt-4">
              <%= @classroom.description %>
          </div>
        </div>
    </div>
  </div>
</div>
