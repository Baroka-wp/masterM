<!-- digitals mentor  display -->
<div class="row justify-content-center p-4">
  <!-- Search form -->
  <form action="/users?category=<%= params[:category]%>&amp;user?LIKE=?" method="get" class="navbar-search navbar-search-light form-inline mr-sm-3" id="navbar-search-main">
    <div class="form-group mb-0">
      <div class="input-group input-group-alternative input-group-merge">
        <div class="input-group-prepend">
          <span class="input-group-text"><%= fa_icon 'search' %></span>
        </div>
        <input class="form-control" placeholder="Search" type="text">
      </div>
    </div>
    <button type="button" class="close" data-action="search-close" data-target="#navbar-search-main" aria-label="Close">
      <span aria-hidden="true">×</span>
    </button>
  </form>

  <div class="col-xl-6 col-lg-6 col-md-8 px-5">
    <h1 class="text-orange"><%= @category_name %>
      <a href="<%= components_classrooms_path %>" class="hover">
      </a><br />
      <span class="text-white text-sm bg-cyan shadow rounded p-1"> Master Class > </span>

    </h1>
    <p class="text-lead text-primary"> Profitez du premier reseau de mentoring pour realisez vos rêves !</p>
  </div>
</div>
  <div class="row">
    <% @users.each do |mentor| %>
    <div class="col-lg-4 col-md-6">
      <%= render "users/shared/index", mentor: mentor %>
    </div>
    <% end %>
  </div>
    <nav aria-label="Page navigation example">
      <ul class="pagination justify-content-center">
        <li class="page-item disabled">
          <a class="page-link" href="#" tabindex="-1">
            <i class="fa fa-angle-left"></i>
            <span class="sr-only">Previous</span>
          </a>
        </li>
        <li class="page-item"><a class="page-link" href="/users?category=<%= params[:category]%>&amp;page=1">1</a></li>
        <li class="page-item"><a class="page-link" href="/users?category=<%= params[:category]%>&amp;page=2">2</a></li>
        <li class="page-item"><a class="page-link" href="/users?category=<%= params[:category]%>&amp;page=3">3</a></li>
        <li class="page-item">
          <a class="page-link" href="#">
            <i class="fa fa-angle-right"></i>
            <span class="sr-only">Next</span>
          </a>
        </li>
      </ul>
    </nav>
</div>
<!--mentor card -->
<!--  resources display -->
<div class="section" style="background-image: url(<%= asset_path 'img/ill/1.svg' %>);">
  <div class="container py-md">
    <div class="row justify-content-between align-items-center">
      <div class="col-lg-6 mb-lg-auto">
        <div class="rounded overflow-hidden transform-perspective-left">
          <div id="carousel_example" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
              <li data-target="#carousel_example" data-slide-to="0" class="active"></li>
              <li data-target="#carousel_example" data-slide-to="1"></li>
              <li data-target="#carousel_example" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img class="img-fluid" src="<%= asset_path 'img/theme/img-1-1200x1000.jpg' %>" alt="First slide">
              </div>
              <div class="carousel-item">
                <img class="img-fluid" src="<%= asset_path 'img/theme/img-2-1200x1000.jpg' %>" alt="Second slide">
              </div>
              <div class="carousel-item">
                <img class="img-fluid" src="<%= asset_path 'img/theme/img-1-1200x1000.jpg' %>" alt="Third slide">
              </div>
            </div>
            <a class="carousel-control-prev" href="#carousel_example" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carousel_example" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
        </div>
      </div>
      <div class="col-lg-5 mb-5 mb-lg-0">
        <h1 class="font-weight-light">ACCEDER AUX MASTER CLASS GRATUITES </h1>
        <p class="lead mt-4">Master Mind met egalement a votre disposition plusieurs ressources de ebook pour vous auto former.
         </p>
        <a href="<%= components_classrooms_path %>">VOIR PLUS</a>
      </div>
    </div>
  </div>
</div>
<!-- end resources display -->
